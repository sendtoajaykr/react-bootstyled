{
  "name": "react-bootstyled",
  "version": "0.1.2",
  "description": "Bootstrap in styled components",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "pre": "npm run clean && babel src -d dist --copy-files",
    "clean": "rm -rf dist",
    "start": "webpack-dev-server --mode development --config webpack.config.js",
    "cover": "jest --coverage --coverageReporters html lcov && codecov",
    "test": "jest"
  },
  "keywords": [
    "components",
    "react",
    "react-component",
    "ui",
    "design-system",
    "styled-system",
    "styled-components",
    "presentational",
    "stateless",
    "functional"
  ],
  "author": "Ajay Kumar",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-rename-import": "^2.2.0",
    "babel-preset-es2015": "^6.24.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "jest-styled-components": "^6.2.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1",
    "styled-components": "^4.0.0-beta.8",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sendtoajaykr/react-bootstyled.git"
  },
  "bugs": {
    "url": "https://github.com/sendtoajaykr/react-bootstyled/issues"
  },
  "homepage": "https://github.com/sendtoajaykr/react-bootstyled#readme",
  "jest": {
    "testMatch": [
      "**/test/**/*.js"
    ],
    "setupTestFrameworkScriptFile": "jest-styled-components",
    "coverageReporters": [
      "html",
      "lcov"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
